
<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put,id:"formalbumprive" }) do |f| %>
  <%= render "users/shared/error_messages", resource: resource %>
  <div class="champ">
    <%=f.file_field :ajouterprive,style: "display:none;",onchange:"formalbumprive.submit()"%>
    <a id="ajouteralbumprive" onclick="user_ajouterprive.click()">+ Ajouter</a>
  </div>
  <div class="champ">
  <%=f.fields_for :photoprives do |b|%>
    <div class="myitem">
    <img src="/uploads/<%=b.object.image%>" width=100 height=100 />
    Delete: <%= b.check_box :_destroy,id:"destroyphotoprive#{b.object.id}",style:"display:none;" %>
  <span onclick="destroyphotoprive<%=b.object.id%>.checked=true;formalbumprive.submit();">supprimer x</span>
    </div>
  <%end%>
  </div>


<% end %>
